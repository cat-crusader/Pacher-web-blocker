{
  "name": "Web blocker",
  "description": "Websites blocker extension for Google Chrome",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts":[
    {
      "matches": ["<all_urls>"],
      "js":["blocker.js"]
    }
  ],
  "permissions":[
    "tabs"
  ],
 "action": {
    "default_popup": "popup.html"
    }
}